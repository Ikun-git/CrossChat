{
  "name": "crosschat",
  "version": "1.0.0",
  "description": "A bridge between Discord and LINE messaging platforms",
  "main": "index.js",
  "scripts": {
    "start": "concurrently -p \"[{name}]\" -n \"Discord,Line\" -c \"#7289da,#06c755\" \"node LineBotSide.js\" \"node DiscordBotSide.js\"",
    "dev": "concurrently -p \"[{name}]\" -n \"Discord,Line\" -c \"#7289da,#06c755\" \"nodemon LineBotSide.js\" \"nodemon DiscordBotSide.js\""
  },
  "keywords": [
    "line",
    "discord",
    "webhook",
    "bot",
    "messaging"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.0",
    "chalk": "^4.1.2",
    "cli-table3": "^0.6.3",
    "concurrently": "^9.1.2",
    "discord-webhook-node": "^1.1.8",
    "discord.js": "^14.18.0",
    "dotenv": "^16.4.7",
    "express": "^4.18.2",
    "figlet": "^1.6.0",
    "inquirer": "^8.2.5",
    "linebot": "^1.6.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
